<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--  Constant  -->
    <system:String x:Key="Constant.NeweastVersionInfo">恭喜您, 当前为最新版本!</system:String>
    <system:String x:Key="Constant.Loading">加载中...</system:String>
    <system:String x:Key="Constant.Unloading">加载结束...</system:String>
    <system:String x:Key="Constant.PlaceHolderContent">Enter 翻译/缓存/nCtrl+Enter 强制翻译/nShift+Enter 换行</system:String>
    <system:String x:Key="Constant.InputErrorContent">该服务未获取到缓存Ctrl+Enter更新</system:String>
    <system:String x:Key="Constant.HistoryErrorContent">该服务翻译时未正确返回Ctrl+Enter以更新</system:String>

    <!--  Enum  -->
    <system:String x:Key="LangEnum.auto">自动识别</system:String>
    <system:String x:Key="LangEnum.zh_cn">中文</system:String>
    <system:String x:Key="LangEnum.zh_tw">繁体中文</system:String>

    <!--  通用文本  -->
    <system:String x:Key="Common_OK">确定</system:String>
    <system:String x:Key="Common_Cancel">取消</system:String>
    <system:String x:Key="Common_Close">关闭</system:String>
    <system:String x:Key="Common_Save">保存</system:String>

    <!--  设置相关  -->
    <system:String x:Key="Settings_Title">设置</system:String>
    <system:String x:Key="Settings_General">常规</system:String>
    <system:String x:Key="Settings_Language">语言</system:String>
    <system:String x:Key="Settings_AutoStart">开机自启动</system:String>
    <system:String x:Key="Settings_AdminStart">管理员启动</system:String>
    <system:String x:Key="Settings_Theme">主题</system:String>
    <system:String x:Key="Settings_FollowMouse">跟随鼠标</system:String>
    <system:String x:Key="Settings_History">历史记录大小</system:String>

    <!--  OCR相关  -->
    <system:String x:Key="OCR_Title">OCR识别</system:String>
    <system:String x:Key="OCR_AutoCopy">自动复制文本</system:String>
    <system:String x:Key="OCR_CloseAfterTranslate">翻译后关闭OCR界面</system:String>
    <system:String x:Key="OCR_ShowAuxiliaryLine">显示辅助线</system:String>

    <!--  翻译相关  -->
    <system:String x:Key="Translate_SourceLang">原始语言</system:String>
    <system:String x:Key="Translate_TargetLang">目标语言</system:String>
    <system:String x:Key="Translate_Auto">自动检测</system:String>
    <system:String x:Key="Translate_Execute">翻译</system:String>
    <system:String x:Key="Translate_Clear">清空</system:String>

    <!--  代理设置  -->
    <system:String x:Key="Proxy_Title">代理设置</system:String>
    <system:String x:Key="Proxy_Method">代理方式</system:String>
    <system:String x:Key="Proxy_System">系统代理</system:String>
    <system:String x:Key="Proxy_None">不使用代理</system:String>
    <system:String x:Key="Proxy_Custom">自定义代理</system:String>

    <!--  其他功能  -->
    <system:String x:Key="Feature_MouseHook">鼠标划词</system:String>
    <system:String x:Key="Feature_Screenshot">截图翻译</system:String>
    <system:String x:Key="Feature_OCR">OCR识别</system:String>
    <system:String x:Key="Feature_QRCode">二维码识别</system:String>
    <system:String x:Key="Feature_History">历史记录</system:String>
</ResourceDictionary>